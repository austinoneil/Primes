<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.primes"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.primes.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.primes.AboutThisApp"
            android:label="@string/title_activity_about_this_app" >
        </activity>
        <activity
            android:name="com.example.primes.IsItPrime"
            android:label="@string/title_activity_is_it_prime" >
        </activity>
        <activity
            android:name="com.example.primes.DisplayMessageActivity"
            android:label="@string/title_activity_display_message" >
        </activity>
        <activity
            android:name="com.example.primes.FirstNPrimes"
            android:label="@string/title_activity_first_nprimes" >
        </activity>
        <activity
            android:name="com.example.primes.PrimeFactorization"
            android:label="@string/title_activity_prime_factorization" >
        </activity>
    </application>

</manifest>